datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  directUrl         = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Order {
  id        Int     @id @default(autoincrement())
  chair     String
  table     String
  arrangement String
  length    Int
  width     Int
  location  String
  date      String
  time      String
  noOfArrangements Int
}

